command = /usr/local/bin/fish

theme = dark:Hipster Green, light:Hipster Green
#
# https://github.com/be5invis/Iosevka/
font-family = "Iosevka Term Curly"

font-family = "Martian Mono Cn Md"

font-size = 15
adjust-cell-height = 20%

shell-integration = fish
shell-integration-features = title

macos-titlebar-style = tabs
macos-option-as-alt = true

term = xterm-256color

window-height = 40
window-width = 132

window-padding-x = 5
window-padding-y = 2

copy-on-select = clipboard

# vim: ts=4 sw=4
